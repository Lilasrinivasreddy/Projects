# Standardize grouped_columns
df_tbl_hist_rec['grouped_columns'] = df_tbl_hist_rec['grouped_columns'].astype(str)
df_tbl_latest_rec['grouped_columns'] = df_tbl_latest_rec['grouped_columns'].astype(str)



self.logger.info(f"Data types - Historical:\n{df_tbl_hist_rec.dtypes}")
self.logger.info(f"Data types - Latest:\n{df_tbl_latest_rec.dtypes}")
