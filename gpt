SELECT rpt.*, 
       mtd.data_lob, mtd.data_dmn, mtd.data_sub_dmn, 
       mtd.db_name, mtd.src_tbl, mtd.meas_name,
       mtd.data_src, mtd.dq_pillar, mtd.opsgenie_api_key, 
       mtd.is_opsgenie_flg, mtd.jira_assignee
FROM vz-it-np-izcv-dev-idmcdo-0.dga_dq_tbls.dqaas_profile_mtd mtd 
INNER JOIN 
    (SELECT * FROM vz-it-np-izcv-dev-idmcdo-0.dga_dq_tbls.dqaas_profile_rpt 
     WHERE rpt_ref_key = '20250305023008083003') rpt
ON mtd.prfl_id = rpt.prfl_id
WHERE UPPER(mtd.prfl_type) = 'CUSTOM_RULES'
