SELECT meas_dt, SUM(cnt) AS meas_value, current_timestamp load_ts FROM ( SELECT CAST(rpt_mth AS DATE) AS meas_dt, CASE WHEN total_gnb IS NOT NULL THEN 0 else 1 END cnt FROM vz-it-pr-gudv-dtwndo-0.aid_dtwin_network_analytics_tbls.network_market_insights_norm WHERE cast(rpt_mth as date) = '2025-03-01') GROUP BY 1
